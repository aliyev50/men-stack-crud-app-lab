<!-- views/foods/index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Foods Index</title>
</head>
<body>
    <h1>All Foods</h1>
    <a href="/foods/new">Add New Foods</a>
    <ul>
        <% foods.forEach(food => { %>
          <li><%= food.name %></li>
        <% }); %>
      </ul>
</body>
</html>
